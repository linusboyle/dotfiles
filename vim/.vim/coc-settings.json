{
    "suggest.noselect":true,
    "suggest.minTriggerInputLength": 2,
    "suggest.floatEnable": true,
    "coc.source.omni.filetypes": [],
    "languageserver":{
        "bash": {
            "command": "bash-language-server",
            "args":["start"],
            "filetypes":["sh"],
            "ignoredRootPaths":["~"]
        },
        "ocaml-lsp": {
            "command": "opam",
            "args": ["config", "exec", "--", "ocamllsp"],
            "filetypes": ["ocaml"]
        }
    }
}
